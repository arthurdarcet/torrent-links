{
    "name": "Add torrents to rtorrent",
    "description": "Downloads torrents using rTorrent on a remote server.",
    "version": "2.1",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [{
        "js": ["tr_links.user.js"],
        "matches": [
            "*://*/*"
        ]
    }],
    "permissions": ["https://torrent.darcet.fr/*"]
}


